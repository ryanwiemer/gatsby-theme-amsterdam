  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "babel-plugin-styled-components": "^1.10.7",
    "framer-motion": "^5.3.0",
    "gatsby": "^4.1.4",
    "gatsby-awesome-pagination": "^0.3.5",
